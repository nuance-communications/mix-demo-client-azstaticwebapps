{
    "routes": [
        {
            "route": "/app/*"
        },
        {
            "route": "/api/*",
            "methods": ["POST"],
        },
        {
            "route": "/"
        },
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/stylesheets/*"]
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}
